{
  "name": "obsidian-emoji-toolbar",
  "version": "0.2.0",
  "description": "Quickly search for an insert emojis into your editor.",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliveryh/obsidian-emoji-toolbar"
  },
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js",
    "build:deploy": "rollup --config rollup.config.js && cp main.js ../../DOCS/.obsidian/plugins/obsidian-emoji-toolbar/main.js"
  },
  "keywords": [
    "obsidian",
    "obsidian-md",
    "obsidian-md-plugin"
  ],
  "author": "oliveryh",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/node": "^14.14.37",
    "@types/twemoji": "^12.1.1",
    "obsidian": "https://github.com/obsidianmd/obsidian-api/tarball/master",
    "rollup": "^2.44.0",
    "tslib": "^2.1.0",
    "twemoji": "^13.0.2",
    "typescript": "^4.2.3",
    "unicode-emoji-json": "^0.2.1"
  }
